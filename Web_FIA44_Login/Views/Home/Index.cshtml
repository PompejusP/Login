@{
		ViewData["Title"] = "Home";

}
<h1 class="display-4">Startseite</h1>

<a class="btn btn-secondary" asp-controller="Home" asp-action="Oeffentlich" >Für Alle!</a>
<a class="btn btn-secondary" asp-controller="Home" asp-action="Intern">Für Mitglieder!</a>
@if (ViewBag.SessionInhalt == null)
{
	<a class="btn btn-secondary" asp-controller="Home" asp-action="Login">Login</a>
}
else
{
	<a class="btn btn-secondary" asp-controller="Home" asp-action="Logout">Logout</a>
}


